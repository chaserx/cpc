{
  "ruby": {
    "command": "bundle",
    "args": ["exec", "ruby-lsp"],
    "description": "Ruby Language Server Protocol (LSP) for code analysis, autocompletion, and error checking.",
    "extensionToLanguage": {
      ".rb": "ruby",
      ".erb": "erb",
      ".rake": "ruby",
      ".gemspec": "ruby",
      ".ru": "ruby"
    },
    "transport": "stdio",
    "initializationOptions": {},
    "settings": {},
    "maxRestarts": 3
  }
}
